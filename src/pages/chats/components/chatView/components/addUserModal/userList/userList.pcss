@import url("../../../../../../../styles/variables.pcss");

.user-list {
    display: flex;
    flex-direction: column;
    gap: 4px;
    align-items: flex-start;
    border-right: 1px solid $grey;
    border-left: 1px solid $grey;
    margin-bottom: 15px;

    &__item {
        width: 100%;
        padding: 5px;
        border-bottom: 1px solid $grey;
        box-sizing: border-box;
    }
}
